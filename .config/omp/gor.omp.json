{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "_color": [
        {
          "type": "text",
          "foreground": "black",
          "template": "black"
        },
        {
          "type": "text",
          "foreground": "green",
          "template": "green"
        },
        {
          "type": "text",
          "foreground": "red",
          "template": "red"
        },
        {
          "type": "text",
          "foreground": "yellow",
          "template": "yellow"
        },
        {
          "type": "text",
          "foreground": "blue",
          "template": "blue"
        },
        {
          "type": "text",
          "foreground": "magenta",
          "template": "magenta"
        },
        {
          "type": "text",
          "foreground": "cyan",
          "template": "cyan"
        },
        {
          "type": "text",
          "foreground": "white",
          "template": "white"
        },
        {
          "type": "text",
          "foreground": "darkGray",
          "template": "darkGray"
        },
        {
          "type": "text",
          "foreground": "lightRed",
          "template": "lightRed"
        },
        {
          "type": "text",
          "foreground": "lightGreen",
          "template": "lightGreen"
        },
        {
          "type": "text",
          "foreground": "lightYellow",
          "template": "lightYellow"
        },
        {
          "type": "text",
          "foreground": "lightBlue",
          "template": "lightBlue"
        },
        {
          "type": "text",
          "foreground": "lightMagenta",
          "template": "lightMagenta"
        },
        {
          "type": "text",
          "foreground": "lightCyan",
          "template": "lightCyan"
        },
        {
          "type": "text",
          "foreground": "lightWhite",
          "template": "lightWhite"
        }
      ],
      "alignment": "left",
      "segments": [
        {
          "foreground": "blue",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "powerline",
          "template": "\uf114  {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "green",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "[{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }}{{ .Full }} \ue718 ]",
          "type": "node"
        },
        {
          "foreground": "magenta",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": "[{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 ]",
          "type": "python"
        },
        {
          "foreground": "lightBlue",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "[{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 ]",
          "type": "go"
        },
        {
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "foreground": "green",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "[ {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ]",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "blue",
          "invert_powerline": true,
          "style": "plain",
          "template": "[{{ .FormattedMs }}]",
          "type": "executiontime",
          "properties": {
            "threshold": 5000,
            "style": "galveston"
          }
        },
        {
          "foreground": "green",
          "foreground_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "invert_powerline": true,
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "[{{ if gt .Code 0 }}{{ reason .Code }}{{ else }}Ok{{ end }}]",
          "type": "status"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .UserName }}@{{ .HostName }} at {{ .PWD }}",
  "final_space": true,
  "version": 2
}
